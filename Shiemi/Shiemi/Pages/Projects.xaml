<?xml version="1.0" encoding="utf-8" ?>
<ContentPage
    x:Class="Shiemi.Pages.Projects"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:pagemodel="clr-namespace:Shiemi.PageModels"
    Title="{Binding Title}"
    x:DataType="pagemodel:ProjectsPageModel">
    <Grid Margin="10" RowDefinitions="*,*,50">
        <Button
            Grid.Row="2"
            Command="{Binding GoToCreateProjectCommand}"
            Text="new" />
    </Grid>
</ContentPage>
