<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="Shiemi.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:index="clr-namespace:Shiemi.Pages"
    Shell.FlyoutBehavior="Flyout">

    <Tab>
        <ShellContent ContentTemplate="{DataTemplate index:Index}" Route="Index" />
    </Tab>

    <FlyoutItem Title="Profile">
        <Tab>
            <ShellContent ContentTemplate="{DataTemplate index:Profile}" Route="Profile" />
        </Tab>
    </FlyoutItem>

    <FlyoutItem Title="Projects">
        <Tab>
            <ShellContent ContentTemplate="{DataTemplate index:Projects}" Route="Projects" />
        </Tab>
    </FlyoutItem>

</Shell>
